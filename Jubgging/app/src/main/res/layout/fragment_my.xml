<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="my"
            type="com.example.jubgging.view.MyFragment" />
        <variable
            name="userVm"
            type="com.example.jubgging.viewmodel.UserInfoViewModel" />
