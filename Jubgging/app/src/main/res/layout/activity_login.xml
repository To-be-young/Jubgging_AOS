<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_grey"
    tools:context=".LoginActivity">


    <TextView
        android:id="@+id/login_title_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36.7dp"
        android:text="@string/login_title"
        android:textColor="#000000"
        android:textSize="34.7sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/login_sub_title_txt"
        android:layout_width="110dp"
        android:layout_height="16.3dp"
        android:layout_marginTop="3.3dp"
        android:fontFamily="@font/nanumgothic_regular"
        android:text="@string/login_subtitle"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_title_txt" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/login_login_view"
        android:layout_width="391.7dp"
        android:layout_height="256.7dp"
        android:layout_marginTop="36.7dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/login_layount_shape"
        app:layout_constraintTop_toBottomOf="@+id/login_sub_title_txt">

        <TextView
            android:id="@+id/login_loginEmail_txt"
            android:layout_width="47dp"
            android:layout_height="19dp"
            android:layout_marginLeft="37.1dp"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/nanumgothic_regular"
            android:text="@string/login_email_txt"
            android:textSize="16.7sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/login_login_Email_ed"
            android:layout_width="317dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="72dp"
            android:hint=""
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/login_login_Password_txt"
            android:layout_width="65dp"
            android:layout_height="19dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="152dp"
            android:fontFamily="@font/nanumgothic_regular"
            android:text="@string/login_password_txt"
            android:textSize="16.7sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/login_login_Password_ed"
            android:layout_width="317dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="184dp"
            android:hint=""
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/login_findID_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10.3dp"
        android:layout_marginRight="8.1dp"
        android:text="@string/login_findID"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/login_slash_txt"
        app:layout_constraintTop_toBottomOf="@+id/login_login_view" />

    <TextView
        android:id="@+id/login_slash_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11.4dp"
        android:layout_marginRight="7.3dp"
        android:text="/"
        android:textSize="11.3sp"
        app:layout_constraintEnd_toStartOf="@+id/login_findPW_txt"
        app:layout_constraintTop_toBottomOf="@+id/login_login_view" />

    <TextView
        android:id="@+id/login_findPW_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10.3dp"
        android:layout_marginRight="50.6dp"
        android:text="@string/login_findPW"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_login_view" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_login_btn"
        android:layout_width="391.7dp"
        android:layout_height="51.7dp"
        android:background="@drawable/login_login_btn_shape"
        android:textSize="19.7sp"
        android:textColor="@color/white"
        android:fontFamily="@font/nanumgothic_bold"
        android:text="@string/login_loginbtn"
        android:layout_marginTop="82.3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_login_view" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_register_btn"
        android:layout_width="391.7dp"
        android:layout_height="51.7dp"
        android:background="@drawable/login_layount_shape"
        android:textSize="19.7sp"
        android:textColor="@color/green_blue"
        android:fontFamily="@font/nanumgothic_bold"
        android:text="@string/login_registerbtn"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_login_btn" />

    <View
        android:id="@+id/login_boundary_view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#1A000000"
        android:layout_marginTop="43.3dp"
        android:layout_marginLeft="44.1dp"
        android:layout_marginRight="44.2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_register_btn"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_kakao_login_btn"
        android:layout_width="391.7dp"
        android:layout_height="51.7dp"
        android:background="@drawable/login_kakao_login_btn"
        android:textSize="19.7sp"
        android:textColor="#000000"
        android:fontFamily="@font/nanumgothic_bold"
        android:text="@string/login_kakao_login"
        android:layout_marginTop="43.3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_boundary_view" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_naver_login_btn"
        android:layout_width="391.7dp"
        android:layout_height="51.7dp"
        android:background="@drawable/login_naver_login_btn"
        android:textSize="19.7sp"
        android:textColor="#ffffff"
        android:fontFamily="@font/nanumgothic_bold"
        android:text="@string/login_naver_login"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_kakao_login_btn" />


</androidx.constraintlayout.widget.ConstraintLayout>