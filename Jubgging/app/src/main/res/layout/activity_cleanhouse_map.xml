<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--tools:context=".view.Cleanhouse_map_Activity"-->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_banner_cleanhousemap"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@drawable/ic_banner_background_drawable"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@+id/page_cleanhousemap"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/page_back_btn"
            android:layout_width="10.2dp"
            android:layout_height="0dp"
            android:layout_marginLeft="25dp"
            android:background="@drawable/ic_back_page"
            app:layout_constraintBottom_toBottomOf="@id/page_title_cleanhousemap"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/page_title_cleanhousemap"
            app:layout_constraintTop_toTopOf="@id/page_title_cleanhousemap" />

        <TextView
            android:id="@+id/page_title_cleanhousemap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="내 주변 클린하우스"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/page_back_btn"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_cleanhousemap"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/light_grey"
        android:paddingHorizontal="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/page_banner_cleanhousemap">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/page_text_field_setting"
        android:layout_width="65dp"
        android:layout_height="19dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="@font/nanumgothic_bold"
        android:text="반경 설정"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@id/page_button_field_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/page_banner_cleanhousemap" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_button_field_1"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/cleanhouse_field_btn_background_drawable"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@id/page_screen_cleanhousemap"
        app:layout_constraintLeft_toLeftOf="@id/page_cleanhousemap"
        app:layout_constraintRight_toLeftOf="@id/page_button_field_2"
        app:layout_constraintTop_toTopOf="@+id/page_cleanhousemap">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="500m"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_button_field_2"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/cleanhouse_field_btn_background_drawable"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@+id/page_screen_cleanhousemap"
        app:layout_constraintLeft_toRightOf="@id/page_button_field_1"
        app:layout_constraintRight_toLeftOf="@id/page_button_field_3"
        app:layout_constraintTop_toTopOf="@+id/page_cleanhousemap">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1km"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_button_field_3"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/cleanhouse_field_btn_background_drawable"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@+id/page_screen_cleanhousemap"
        app:layout_constraintLeft_toRightOf="@id/page_button_field_2"
        app:layout_constraintRight_toLeftOf="@id/page_button_field_4"
        app:layout_constraintTop_toTopOf="@+id/page_cleanhousemap">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2km"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_button_field_4"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/cleanhouse_field_btn_background_drawable"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@id/page_screen_cleanhousemap"
        app:layout_constraintEnd_toEndOf="@+id/page_screen_cleanhousemap"
        app:layout_constraintLeft_toRightOf="@id/page_button_field_3"
        app:layout_constraintRight_toRightOf="@id/page_cleanhousemap"
        app:layout_constraintTop_toTopOf="@id/page_cleanhousemap">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3km"
            android:textColor="#000000"
            android:textSize="13sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_screen_cleanhousemap"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="25dp"
        android:layout_marginTop="109dp"
        android:layout_marginBottom="29.8dp"
        android:background="@drawable/screen_cleanhousemap"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/page_cleanhousemap">
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/page_map_cleanhousemap"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="9dp"
        android:background="@drawable/screen_cleanhousemap"
        app:layout_constraintBottom_toBottomOf="@id/page_screen_cleanhousemap"
        app:layout_constraintLeft_toLeftOf="@id/page_screen_cleanhousemap"
        app:layout_constraintRight_toRightOf="@id/page_screen_cleanhousemap"
        app:layout_constraintTop_toTopOf="@id/page_screen_cleanhousemap" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_switch_btn_cleanhousemap"
        android:layout_width="73dp"
        android:layout_height="41dp"
        android:layout_marginTop="16dp"
        android:layout_marginRight="14dp"
        android:background="@drawable/cleanhouse_switch_background_drawable"
        android:elevation="100dp"
        app:layout_constraintRight_toRightOf="@id/page_screen_cleanhousemap"
        app:layout_constraintTop_toTopOf="@id/page_screen_cleanhousemap"
        >

        <Switch
            android:id="@+id/page_switch_btn_cleanhousemap_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:track="@drawable/cleanhouse_switch_track_selector"
            android:thumb="@drawable/cleanhouse_switch_thumb_selector"
            app:layout_constraintTop_toTopOf="@id/page_switch_btn_cleanhousemap"
            app:layout_constraintRight_toRightOf="@id/page_switch_btn_cleanhousemap"
            app:layout_constraintBottom_toBottomOf="@id/page_switch_btn_cleanhousemap"
            app:layout_constraintLeft_toLeftOf="@id/page_switch_btn_cleanhousemap"
            >
        </Switch>
        <TextView
            android:id="@+id/page_switch_btn_cleanhousemap_text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="On"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#ffffff"
            android:gravity="center_horizontal"
            android:fontFamily="@font/nanumgothic_bold"
            app:layout_constraintTop_toTopOf="@id/page_switch_btn_cleanhousemap_switch"
            app:layout_constraintRight_toLeftOf="@id/page_switch_btn_cleanhousemap_text2"
            app:layout_constraintBottom_toBottomOf="@id/page_switch_btn_cleanhousemap_switch"
            app:layout_constraintLeft_toLeftOf="@id/page_switch_btn_cleanhousemap_switch"
            >
        </TextView>
        <TextView
            android:id="@+id/page_switch_btn_cleanhousemap_text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="OFF"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#ffffff"
            android:gravity="center_horizontal"
            android:fontFamily="@font/nanumgothic_bold"
            app:layout_constraintTop_toTopOf="@id/page_switch_btn_cleanhousemap_switch"
            app:layout_constraintRight_toRightOf="@id/page_switch_btn_cleanhousemap_switch"
            app:layout_constraintBottom_toBottomOf="@id/page_switch_btn_cleanhousemap_switch"
            app:layout_constraintLeft_toRightOf="@id/page_switch_btn_cleanhousemap_text1"
            >
        </TextView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:elevation="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintRight_toRightOf="@id/page_map_cleanhousemap"
        app:layout_constraintBottom_toBottomOf="@id/page_map_cleanhousemap">
        <ToggleButton
            android:id="@+id/toggleButton1"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:background="@drawable/ic_cleanhouse_toggle_btn"
            android:text="ToggleButton"
            android:textSize="0dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>